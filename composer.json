{
  "name": "davidcochrum/phpdotenv-vault",
  "description": "Load environment variables from encrypted .env.vault files",
  "keywords": [
    "env",
    "dotenv",
    "environment",
    "environment variables",
    "dotenv-vault",
    "configurations"
  ],
  "license": "BSD-3-Clause",
  "autoload": {
    "psr-4": {
      "DotenvVault\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotenvVault\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "dotenv",
      "email": "mot@dotenv.org",
      "homepage": "https://github.com/dotenv-org"
    },
    {
      "name": "David COchrum",
      "email": "david@chrum.me",
      "homepage": "https://chrum.me"
    }
  ],
  "replace": {
    "dotenv-org/phpdotenv-vault": "^v0"
  },
  "require": {
    "php": "^7.1.3 || ^8.0",
    "ext-openssl": "*",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^9.0|^8.0|^7.0|6.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
