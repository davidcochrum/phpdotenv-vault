{
  "name": "dotenv-org/phpdotenv-vault",
  "description": "Load environment variables from encrypted .env.vault files",
  "keywords": [
    "env",
    "dotenv",
    "environment",
    "environment variables",
    "dotenv-vault",
    "configurations"
  ],
  "license": "BSD-3-Clause",
  "autoload": {
    "psr-4": {
      "DotenvVault\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotenvVaultTest\\": "tests"
    }
  },
  "authors": [
    {
      "name": "dotenv",
      "email": "mot@dotenv.org",
      "homepage": "https://github.com/dotenv-org"
    },
    {
      "name": "David Cochrum",
      "email": "david@chrum.me",
      "homepage": "https://chrum.me"
    }
  ],
  "bin": [
    "dotenv-vault"
  ],
  "require": {
    "php": "^7.1.3 || ^8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "guzzlehttp/guzzle": "^6.5",
    "loilo/native-open": "^1.0",
    "symfony/console": "^4.4",
    "vlucas/phpdotenv": "^5.5"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^7.5"
  }
}
